# Example .env for WearUp Backend
# Copy this to .env in the Backend folder (do NOT commit .env to git)

DJANGO_SECRET_KEY=replace-this-with-a-secure-secret
DJANGO_DEBUG=0
DJANGO_ALLOWED_HOSTS=yourusername.pythonanywhere.com,localhost

DB_ENGINE=django.db.backends.mysql
DB_NAME=yourusername$wearup
DB_USER=yourusername
DB_PASSWORD=your-db-password
DB_HOST=127.0.0.1
DB_PORT=3306
